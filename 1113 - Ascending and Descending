#include <stdio.h>
int main()
{
   
       int m,n,i;
       
       i=0;
       while(i=1)
       {
           scanf("%d %d", &m,&n);
           
           if (n==m)
           {
               break;
           }
           else 
           {
               
               if(m>n)
               {
                 
                   printf("Decrescente\n");
               }
             
               
               else if(n>m)
               {
                   
               
                   printf("Crescente\n");
               }
         
               
           }
               
        }
       
    
    return 0;
}
