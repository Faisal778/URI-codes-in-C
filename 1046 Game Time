#include <stdio.h>

int main()
{
    int x,y;
    
    scanf("%d %d", &x, &y);
    
    if(x>y)
    {
        x=24-x;
        printf("O JOGO DUROU %d HORA(S)\n", x+y);
    }
    
    else if(x==y)
    {
        printf("O JOGO DUROU 24 HORA(S)\n");
    }
    else 
    {
        printf("O JOGO DUROU %d HORA(S)\n", y-x);
    }
    
    return 0;
}
