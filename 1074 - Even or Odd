#include <stdio.h>

int
main ()
{


  int a,i,j;
  scanf("%d", &a);

  for (j = 1; j <=a; j++)
    {
        scanf("%d",&i);
        
        if(i%2 == 1 || i%2==-1)
        {
            if(i<0)
            printf("ODD NEGATIVE\n");
            
        }
        if(i%2==1 || i%2==-1 )
        {
            if( i>0)
            printf("ODD POSITIVE\n");
        }
        
        if(i%2==0 && i<0)
        {
            printf("EVEN NEGATIVE\n");
        }
        if(i%2==0 && i>0)
        {
            printf("EVEN POSITIVE\n");
        }
        if(i==0)
        {
            printf("NULL\n");
        }
        
  
    }
   
  return 0;
}
