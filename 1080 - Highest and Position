#include <stdio.h>
int main()
{
   
       int i,n,max=0, max_pos=0;
  
    for(i=1; i<=100; i++)
    
        { 
        scanf("%d",&n);
         if(n>max)
        { max = n;
        max_pos=i;
        }
                 
        }
       printf("%d\n",max);
    printf("%d\n",max_pos);
    
    return 0;
}
