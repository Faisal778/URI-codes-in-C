#include <stdio.h>
int main()
{
   
       int m,n,i,j,sum=0;
       
       i=0;
       while(i=1)
       {
           scanf("%d %d", &m,&n);
           
           if (n<=0 || m<=0)
           {
               break;
           }
           else 
           {
               
               if(m>n)
               {
                  sum=n;
               for(j=n; j<=m; j++)
               {
                  
                   sum=sum+j;
                   printf("%d ",j);
               }
               printf("Sum=%d\n",sum-n);
               }
               else if(n>m)
               {
                   
                     sum=m;
               for(j=m; j<=n; j++)
               {
                  
                   sum=sum+j;
                   printf("%d ",j);
               }
               printf("Sum=%d\n",sum-m);
               }
           }
               
           }
       
    
    return 0;
}
