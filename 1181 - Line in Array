#include <stdio.h>

int main()
{
   
 
   double arr[12][12],sum=0.0;
   int i,j,k;
   char c[2];
   
   scanf("%d", &k);
   scanf("%s", &c[0]);
   
  for(i=0; i<12; i++)
  {
       for(j=0; j<12; j++)
       {
            scanf("%lf", &arr[i][j]);
       }
  }
  
  
       for(j=0; j<12; j++)
       {
      
           sum= sum+arr[k][j];
           
       }
 
      if(c[0]=='S')
      {
          printf("%.1lf\n",sum);
      }
      else if(c[0]=='M')
      {
          printf("%.1lf\n", sum/12);
      }
     
    return 0;
}
