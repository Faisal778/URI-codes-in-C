#include <stdio.h>

int main()
{
    double arr[12][12];
    int i,j,l;
    char c[2];
    double k,sum=0.0;
    
    scanf("%d", &l);
    scanf("%s",&c[0]);
    
    for(i=0; i<12; i++)
    {
        for(j=0; j<12; j++)
        {
            scanf("%lf",&arr[i][j]);
        }
    }
    
    for(i=0; i<12; i++)
    {
         sum=sum+arr[i][l];
       
    }
     if(c[0]=='S')
        {
            printf("%.1lf\n",sum);
        }
        else if(c[0]=='M')
        {
            printf("%.1lf\n",sum/12);
        }
}
