#include <stdio.h>

int main()
{
   
 
   int i,j;
   
   i=0;
   while(i=1)
   {
       scanf("%d", &j);
       
       if(j==2002)
       {
           printf("Acesso Permitido\n");
           break;
       }
       else
       {
           printf("Senha Invalida\n");
       }
   }
   
   
    return 0;
}
