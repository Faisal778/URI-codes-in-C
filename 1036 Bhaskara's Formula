#include <stdio.h>
#include <math.h>

int main()
{
   float a,b,c,r1,r2,x,k;

    scanf("%f %f %f", &a, &b, &c);
    x= 2*a;
    k= (b*b-4*a*c);
   r1 = ( -b + sqrt(k))/x;
   r2 = ( -b - sqrt(k))/x;
   
   if (x==0 || k<0 )
   {
       printf("Impossivel calcular\n");
   }
   
   else
   {
       printf("R1 = %.5f\n", r1);
       printf("R2 = %.5f\n", r2);
   }
   
 return 0;
}
